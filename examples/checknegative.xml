<testvar varname="RIMATTEST">http://rimattest.azurewebsites.net</testvar>

<case
    id="10"
    description1="Get Home page"
    method="get"
    url="${RIMATTEST}"
    verifynegative5="WebInject"
/>

<case
    id="20"
    description1="Get Home page again"
    method="get"
    url="${RIMATTEST}"
    checknegative="5"
    verifypositive1="WebInject"
/>

</testcases>