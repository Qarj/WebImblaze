<testcases repeat="1">

<!--
Self test master file. All test steps in this file should pass.
-->

<testvar varname="feature">repeat</testvar>
<case
    id="10"
    description1="${feature}"
    method="cmd"
    command=".\webinject.pl -o selftest\output\${feature}sub selftest\substeps\${feature}.xml"
    verifypositive="Test Cases Failed: 0"
    verifypositive1="Test Cases Passed: 10"
    verifypositive2="loop number 1"
    verifypositive3="loop number 2"
    verifypositive4="loop number 3"
    verifypositive5="loop number 4"
    verifypositive6="loop number 5"
    verifynegative="uninitialized value"
    assertcount="0020.1|||4"
    assertcount1="0020.2|||4"
    assertcount2="- 100|||4"
    assertcount3="- 200|||4"
    assertcount4="- 300|||4"
    assertcount5="- 400|||4"
/>
<!-- 10000 is added to the test step number with every loop -->

</testcases>