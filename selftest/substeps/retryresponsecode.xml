<testcases repeat="1">

<!--
Self test sub step file. Some test steps may fail. 
Expected failures will be asserted by the master test file.
-->

<!-- Note - method of cmd always generates a response code of 100 -->
<case
    id="10"
    description1="Should not retry due to 404 not matching 500"
    method="get"
    url="http://{RAN}/pagedoesnotexist"
    verifypositive="This text will not be found"
    retryresponsecode="500"
    retry="2"
/>

<case
    id="20"
    description1="Should retry due to 404 matching 404"
    method="get"
    url="http://{RAN}/pagedoesnotexist"
    verifypositive="This text will not be found"
    retryresponsecode="404"
    retry="2"
/>

<case
    id="30"
    description1="Should not retry due to 404"
    method="get"
    url="http://{RAN}/pagedoesnotexist"
    verifypositive="This text will not be found"
    retry="2"
/>

